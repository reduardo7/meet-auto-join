{
  "manifest_version": 2,
  "name": "Meet Auto-Join",
  "description": "Auto accept Meet participants",
  "version": "1.1",
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["activeTab", "storage", "declarativeContent"]
}
